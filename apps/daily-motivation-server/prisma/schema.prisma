datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Image {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  updatedAt   DateTime @updatedAt
  url         String?
}

model Quote {
  author    String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  text      String?
  updatedAt DateTime @updatedAt
}
